    <add name="BANKDBCONN" connectionString="server=WEIFI-PC\SQLEXPRESS;integrated security=true;database=MyBank"/>
    <add name="MYSQLBANKDBCONN" connectionString="server=localhost;uid=root;pwd=;database=MyBank"/>
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.0">
